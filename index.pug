doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        
        //- Veb site title
        title UI-KIT-WEB-01
        //- Veb site title

        //- Veb site favicons
        link(rel="shortcut icon", href="../img/FUFU logo.png", type="image/x-icon")
        //- Veb site favicons

        //- Css link
        link(rel="stylesheet", href="../main.css")
        //- Css link
        
        //- Bootstrap icon
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css")
        link(rel="stylesheet", href="ikonlar.css")
