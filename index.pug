    body 
        //- Header start
        header.header
            div.container.header__block
                div.block__items
                    div.items__subtitle
                        h2 FU
                        p FU
                    div.block__line-item
                    p Something about lemon
                div.block__item
                    img(src="../img/Group.png", alt="")
                    p how to eat
        //- Header end

        //- Main start
        main
            section.card-section
                div.container.card-section__items
                    img(src="../img/LEMON.png", alt="")
                    div.card-items__title
                        div.title__text
                            p 1/12
                        div.title__icons
                            i.bi.bi-chevron-left
                            i.bi.bi-chevron-right
                    div.card-items__image-card
                        img.image-card__img(src="../img/cw1MAVs4wV8.png", alt="")
                    div.card-items__scroll
                        div.scroll__black-line
                        div.scroll__gray-line
                        i.bi.bi-search
        //- Main end